{
  "name": "ai-tree",
  "version": "0.1.0",
  "private": true,
  "description": "AI Tree (Dendrix.ai) - Public AI learning platform using tree metaphor",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint app components lib --ext .ts,.tsx",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "paraglide:compile": "paraglide-js compile --project ./project.inlang --outdir ./paraglide",
    "postinstall": "paraglide-js compile --project ./project.inlang --outdir ./paraglide"
  },
  "dependencies": {
    "@inlang/paraglide-next": "^0.8.1",
    "@supabase/supabase-js": "^2.93.3",
    "@vercel/analytics": "^1.6.1",
    "framer-motion": "^11.0.0",
    "lucide-react": "^0.469.0",
    "next": "^16.1.6",
    "next-intl": "^4.7.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@playwright/test": "^1.58.0",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@vitejs/plugin-react": "^5.1.2",
    "autoprefixer": "^10.4.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "15.1.0",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
